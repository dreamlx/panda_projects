<div>
  <table class="table table-striped">
    <thead>
      <tr>
      <% @records.first.keys.each do |key| %>
        <th><%= key %></th>
      <% end %>
      </tr>
    </thead>
    <tbody>
      <% @records.each do |record| %>
        <tr>
        <% record.values.each do |value| %>
          <td><%= value %></td>
        <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
