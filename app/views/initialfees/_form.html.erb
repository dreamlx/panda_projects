<%= simple_form_for(@initialfee) do |f| %>
  <%= f.input :project_id, collection: Project.all %> 
  <%= f.input 'created_on', as: :date, html5: true  %>
  <%= f.input 'updated_on', as: :date, html5: true  %>
  <%= f.input 'service_fee'  %>
  <%= f.input 'commission'  %>
  <%= f.input 'outsourcing'  %>
  <%= f.input 'reimbursement'  %>
  <%= f.input 'travel_allowance'  %>
  <%= f.input 'travel_allowance'  %>
  <%= f.input 'business_tax'  %>
  <%= f.input 'tickets'  %>
  <%= f.input 'courrier'  %>
  <%= f.input 'postage'  %>
  <%= f.input 'stationery'  %>
  <%= f.input 'report_binding'  %>
