<h1>Listing initialfees</h1>
<div>
  <%= link_to 'New', new_initialfee_path %>
</div>
<%= paginate @initialfees %>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= sort_link(@q, :project_job_code) %></th>
      <th><%= sort_link(@q, :service_fee) %></th>
      <th><%= sort_link(@q, :commission) %></th>
      <th><%= sort_link(@q, :outsourcing) %></th>
      <th><%= sort_link(@q, :reimbursement) %></th>
      <th><%= sort_link(@q, :meal_allowance) %></th>
      <th><%= sort_link(@q, :travel_allowance) %></th>
      <th><%= sort_link(@q, :business_tax) %></th>
      <th><%= sort_link(@q, :tickets) %></th>
      <th><%= sort_link(@q, :courrier) %></th>
      <th><%= sort_link(@q, :postage) %></th>
      <th><%= sort_link(@q, :stationery) %></th>
      <th><%= sort_link(@q, :report_binding) %></th>
      <th><%= sort_link(@q, :payment_on_be_half) %></th>
      <th colspan="3">Operation</th>
    </tr>
  </thead>
  <tbody>
    <%= render @initialfees %>
  </tbody>
</table>
<%= paginate @initialfees %>